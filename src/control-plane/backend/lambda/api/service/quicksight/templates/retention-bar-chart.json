{
    "BarChartVisual":
    {
    "VisualId":"{{visualId}}",
    "Title":{
        "FormatText": {
            "PlainText": "{{title}}"
        },
        "Visibility":"VISIBLE"
    },
    "Subtitle":{
        "FormatText": {
            "PlainText": "{{subTitle}}"
        },
        "Visibility":"VISIBLE"
    },
    "ChartConfiguration":{
        "FieldWells":{
            "BarChartAggregatedFieldWells":{
                "Category":[
                    {
                        "DateDimensionField":{
                            "FieldId":"{{dateDimFieldId}}",
                            "Column":{
                                "DataSetIdentifier":"{{dataSetIdentifier}}",
                                "ColumnName":"event_date"
                            }
                        }
                    }
                ],
                "Values":[
                    {
                        "NumericalMeasureField":{
                            "FieldId":"{{numberMeasureFieldId}}",
                            "Column":{
                                "DataSetIdentifier":"{{dataSetIdentifier}}",
                                "ColumnName":"retention"
                            },
                            "AggregationFunction":{
                                "SimpleNumericalAggregation":"SUM"
                            }
                        }
                    }
                ],
                "Colors":[
                    {
                        "CategoricalDimensionField":{
                            "FieldId":"{{catDimFieldId}}",
                            "Column":{
                                "DataSetIdentifier":"{{dataSetIdentifier}}",
                                "ColumnName":"grouping"
                            }
                        }
                    }
                ]
            }
        },
        "SortConfiguration":{
            "CategorySort":[
                {
                    "FieldSort":{
                        "FieldId":"{{dateDimFieldId}}",
                        "Direction":"ASC"
                    }
                }
            ],
            "CategoryItemsLimit":{
                "OtherCategories":"INCLUDE"
            },
            "ColorItemsLimit":{
                "OtherCategories":"INCLUDE"
            },
            "SmallMultiplesLimitConfiguration":{
                "OtherCategories":"INCLUDE"
            }
        },
        "Orientation":"VERTICAL",
        "BarsArrangement":"CLUSTERED",
        "DataLabels":{
            "Visibility":"HIDDEN",
            "Overlap":"DISABLE_OVERLAP"
        },
        "Tooltip":{
            "TooltipVisibility":"VISIBLE",
            "SelectedTooltipType":"DETAILED",
            "FieldBasedTooltip":{
                "AggregationVisibility":"HIDDEN",
                "TooltipTitleType":"PRIMARY_VALUE",
                "TooltipFields":[
                    {
                        "FieldTooltipItem":{
                            "FieldId":"{{dateDimFieldId}}",
                            "Visibility":"VISIBLE"
                        }
                    },
                    {
                        "FieldTooltipItem":{
                            "FieldId":"{{numberMeasureFieldId}}",
                            "Visibility":"VISIBLE"
                        }
                    },
                    {
                        "FieldTooltipItem":{
                            "FieldId":"{{catDimFieldId}}",
                            "Visibility":"VISIBLE"
                        }
                    }
                ]
            }
        }
    },
    "Actions":[

    ]
    }
}