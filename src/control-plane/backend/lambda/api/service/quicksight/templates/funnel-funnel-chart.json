{
  "FunnelChartVisual":{
      "VisualId":"{{visualId}}",
      "Title":{
          "FormatText": {
            "PlainText": "{{title}}"
          },
          "Visibility":"VISIBLE"
      },
      "Subtitle":{
          "FormatText": {
            "PlainText": "{{subTitle}}"
          },
          "Visibility":"VISIBLE"
      },
      "ChartConfiguration":{
          "FieldWells":{
              "FunnelChartAggregatedFieldWells":{
                  "Category":[
                      {
                          "CategoricalDimensionField":{
                              "FieldId":"{{dimFieldId}}",
                              "Column":{
                                  "DataSetIdentifier":"{{dataSetIdentifier}}",
                                  "ColumnName":"event_name"
                              }
                          }
                      }
                  ],
                  "Values":[
                      {
                          "CategoricalMeasureField":{
                              "FieldId":"{{measureFieldId}}",
                              "Column":{
                                  "DataSetIdentifier":"{{dataSetIdentifier}}",
                                  "ColumnName":"{{countColName}}"
                              },
                              "AggregationFunction":"DISTINCT_COUNT"
                          }
                      }
                  ]
              }
          },
          "SortConfiguration":{
              "CategorySort":[
                  {
                      "FieldSort":{
                          "FieldId":"{{measureFieldId}}",
                          "Direction":"DESC"
                      }
                  },
                  {
                      "FieldSort":{
                          "FieldId":"{{dimFieldId}}",
                          "Direction":"DESC"
                      }
                  }
              ],
              "CategoryItemsLimit":{
                  "OtherCategories":"INCLUDE"
              }
          },
          "Tooltip":{
              "TooltipVisibility":"VISIBLE",
              "SelectedTooltipType":"DETAILED",
              "FieldBasedTooltip":{
                  "AggregationVisibility":"HIDDEN",
                  "TooltipTitleType":"PRIMARY_VALUE",
                  "TooltipFields":[
                      {
                          "FieldTooltipItem":{
                              "FieldId":"{{dimFieldId}}",
                              "Visibility":"VISIBLE"
                          }
                      },
                      {
                          "FieldTooltipItem":{
                              "FieldId":"{{measureFieldId}}",
                              "Visibility":"VISIBLE"
                          }
                      }
                  ]
              }
          },
          "DataLabelOptions":{
              "Visibility":"VISIBLE",
              "CategoryLabelVisibility":"VISIBLE",
              "MeasureLabelVisibility":"VISIBLE"
          }
      },
      "Actions":[

      ],
      "ColumnHierarchies":[

      ]
  }
}